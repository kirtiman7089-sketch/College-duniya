<% layout('layouts/boiler') %>
<h1 class="user1" style="opacity: 0;"><%=user%></h1>
<main>
    <header>
        <div class="title">
            <img src="" width="35px">
            <h1>Welcome to Dicuss Chat App</h1> 
        </div>
        <button type="button">
          <img src="" width="50px">
        </button>
    </header>
    <div class="container">
        <div class="users-window">
           <div class="users-head">
            <h3>People</h3>
            <h4 class="users-count">0</h4>
           </div>
           <div class="users-list">
           </div>
        </div>
        <div class="chat-window">
           <div class="chats">
           </div>
           <div class="user-input">
            <input id="user-msg" type="text" placeholder="Type your message....">
            <button type="click" id="user-send"> Send </button>
           </div>
        </div>
    </div>
</main>
